{
    "name": "thf-angular-crud-templates",
    "publisher": "scartecjv",
    "displayName": "THF Angular CRUD Template for VS CODE",
    "description": "TOTVS HTML Framework Angular CRUD template for VS CODE by TOTVS.",
    "icon": "assets/images/thf.png",
    "galleryBanner": {
        "color": "#333",
        "theme": "dark"
    },
    "license": "SEE LICENSE IN LICENSE",
    "homepage": "https://github.com/totvs/thf-angular-crud-templates",
    "repository": {
        "type": "git",
        "url": "https://github.com/totvs/thf-angular-crud-templates.git"
    },
    "keywords": [
        "ANGULAR",
        "TOTVS",
        "THF ANGULAR",
        "THF ANGULAR CRUD COMPONENT",
        "GENERATE CRUD COMPONENT"
    ],
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.thfCRUDSimple"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.thfCRUDSimple",
                "title": "THF: CRUD Simple"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.thfCRUDSimple",
                    "group": "THF@1"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^10.5.2",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "change-case": "^3.0.0",
        "fs": "0.0.1-security",
        "fs-extra": "^0.30.0",
        "rxjs": "^5.0.0-rc.1"
    }
}
